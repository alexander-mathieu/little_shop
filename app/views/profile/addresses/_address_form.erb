<%= form_for @address, url: form_submit_path do |f| %>
  <p>
    <%= f.label :address %>:
    <%= f.text_field :address %>
  </p>

  <p>
    <%= f.label :city %>:
    <%= f.text_field :city %>
  </p>

  <p>
    <%= f.label :state %>:
    <%= f.text_field :state %>
  </p>

  <p>
    <%= f.label :zip %>:
    <%= f.text_field :zip %>
  </p>

  <p>
    <%= f.label :nickname %>:
    <%= f.text_field :nickname %>
  </p>

  <p>
    <%= f.submit 'Save Address' %>
  </p>
<% end %>
